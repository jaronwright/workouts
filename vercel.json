{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/:path(.*)",
      "has": [{ "type": "host", "value": "workouts.jaronwright.com" }],
      "destination": "https://www.jaronwright.com/:path",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
